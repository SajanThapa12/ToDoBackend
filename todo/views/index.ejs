<!DOCTYPE html>
<html lang="en">
<head>
    <title>Todo List</title>
    <link rel="stylesheet" type="text/css" href="/css/style.css">
</head>
<body>
   <h1>Todo List</h1>
   <p>Hello <%=name%></p>
   <ul>
    <%data.forEach(function(todo){ %>
    <li>
        <!-- <input type="checkbox" name="completed" value="true"> Completed<br> -->
        <%= todo.title %>
        <%= todo.created_at %>
    </li>
 <% }); %>
   </ul>
</body>
</html>